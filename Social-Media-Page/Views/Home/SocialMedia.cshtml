                            alt="Avatar" class="w3-left w3-circle w3-margin-right" style="width:60px">
                        <span class="w3-right w3-opacity">@msg.CreatedAt</span>
                        

                        <div>
                            <h6> @msg.User.FirstName says:</h6>
                            <p>@msg.Text</p>
                            <hr class="w3-clear">
                            
                            <span class="w3-right w3-opacity">
                                <div>
                                    @if (ViewBag.CurrentUser.UserId == msg.User.UserId)
                                    {
                                        <form action="/dashboard/@msg.MessageId/delete" method="post">
                                            <button class="btn btn-danger">Delete</button>
                                        </form>
                                    }
                                    @* else if (msg.Users.Any(p => p.UserId == ViewBag.CurrentUser.UserId))
                                    {
                                        <form action="/activity/@act.ActivityId/leave" method="post">
                                            <button class="btn btn-warning">Leave</button>
                                        </form>
                                    }
                                    else
                                    {
                                        <form action="/media/@msg.MessageId/like" method="post">
                                            <button class="btn btn-primary">Like</button>
                                        </form>
                                    } *@
                                </div>
                            </span>
                        </div>

                        @foreach (var comment in msg.Comments)
                        {
                            @if (comment.Users != null)
                                {
                            <div style="font-weight: bold;">
                                @comment.Users.FirstName replied:
                            </div>
                            }
                            <div>@comment.Comments</div>
                            <hr class="w3-clear">
                        }
                        

                        <form action="/postComment" method="post">
                            <input asp-for="Comments" placeholder="Text" />
                            <input asp-for="MessageId" type="hidden" value="@msg.MessageId" />
                            @* <input asp-for="UserId" type="hidden" value="@ViewBag.CurrentUser.UserId" /> *@
                            
                            <button type="submit" class="w3-button w3-theme-d2 w3-margin-bottom">
                                <i class="fa fa-comment"></i>
                                Comment
                            </button>

                            
                            @* Liking a post *@
                            @* <div class="w3-row-padding" style="margin:0 -16px">
                                <button type="button" class="w3-button w3-theme-d1 w3-margin-bottom"><i class="fa
                                fa-thumbs-up"></i>
                                Like</button>
                                </div>  *@
                            <!-- Add icon library -->
                            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

                            <!-- Use an element to toggle between a like/dislike icon -->
                            <i onclick="myFunction(this)" class="fa fa-thumbs-up"></i>
                        </form>
                    </div>
                }

            </div>
                <!-- trying to add multiple comments -->

                @* <form action="/postComment" method="post">
                            <input asp-for="Comments" placeholder="Text" />
                            <input asp-for="CommentId" type="hidden" value="@ViewBag.AllComments.CommentId" />
                            <input asp-for="UserId" type="hidden" value="@ViewBag.AllComments.User.UserId" />
                            
                            <button type="submit" class="w3-button w3-theme-d2 w3-margin-bottom">
                                <i class="fa fa-comment"></i>
                                Comment on Comment
                            </button>
                </form> *@

            <!-- End Middle Column -->

            <!-- Right Column -->
            <div class="w3-col m2">
                <div class="w3-card w3-round w3-white w3-center">
                    <div class="w3-container">
